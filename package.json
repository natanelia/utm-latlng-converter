{
  "name": "utm-latlng-converter",
  "version": "1.0.0",
  "description": "High-precision UTM to Lat/Lng converter using Karney/KrÃ¼ger 6th-order series",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && cp build/utm.wasm dist/utm.wasm",
    "test": "vitest run",
    "test:wasm": "vitest run --browser.enabled=false src/wasm.test.ts",
    "test:watch": "vitest",
    "bench": "vitest bench --run --browser.enabled=false",
    "bench:serve": "npx serve -p 3000 & cloudflared tunnel --url http://localhost:3000",
    "asbuild": "asc assembly/utm.ts -o build/utm.wasm --optimize --enable simd",
    "asbuild:sab": "asc assembly/utm-sab.ts -o build/utm-sab.wasm --optimize --importMemory --sharedMemory --maximumMemory 256 --enable threads",
    "asbuild:batch": "asc assembly/utm-sab.ts -o build/utm-batch.wasm --optimize",
    "prepublishOnly": "npm run asbuild && npm run test && npm run build"
  },
  "keywords": [
    "utm",
    "latitude",
    "longitude",
    "coordinates",
    "geodesy",
    "wgs84",
    "karney",
    "transverse-mercator"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/natan/utm-converter.git"
  },
  "bugs": {
    "url": "https://github.com/natan/utm-converter/issues"
  },
  "homepage": "https://github.com/natan/utm-converter#readme",
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/browser": "^1.0.0",
    "@webgpu/types": "^0.1.68",
    "assemblyscript": "^0.28.9",
    "geodesy": "^2.4.0",
    "playwright": "^1.40.0",
    "proj4": "^2.20.2",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@assemblyscript/loader": "^0.28.9"
  }
}
